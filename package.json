{
  "name": "@pi-base/compile",
  "version": "1.1.0",
  "private": true,
  "description": "Compile data into a JSON bundle",
  "main": "lib/main.js",
  "bin": "lib/watch.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "release": "ncc build src/main.ts && git add -f dist/index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pi-base/compile.git"
  },
  "keywords": [
    "actions",
    "node",
    "topology"
  ],
  "author": "James Dabbs",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@pi-base/core": "^0.0.4",
    "chalk": "^4.0.0",
    "chokidar": "^3.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "yaml-front-matter": "^4.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.23",
    "@types/node": "^13.7.6",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "theredoc": "^1.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}